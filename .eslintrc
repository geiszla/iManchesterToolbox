{
  "parser": "babel-eslint",
  // "extends": ["standard", "standard-react"],
  "extends": ["airbnb"],
  "rules":{
        "no-console": 0,
        "semi": [2, "always"],
        "comma-dangle": [2, "never"],
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "import/first": 0,
        "no-use-before-define": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "no-underscore-dangle": 0,
        "no-mixed-operators": 0
    }
}
